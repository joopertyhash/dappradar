{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "object-literal-sort-keys": false,
    "curly": false,
    "no-duplicate-imports": true,
    "interface-name": false,
    "no-console": false,
    "member-access": false,
    "no-consecutive-blank-lines": true
  },
  "rulesDirectory": []
}
